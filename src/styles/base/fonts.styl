  font-family: 'DinCyr-Regular';
  src: url(../fonts/DinCyr-Regular.ttf);
  font-weight: normal;
}
@font-face {
  font-family: 'DinCyr-Bold';
  src: url(../fonts/DinCyr-Bold.ttf);
  font-weight: bold;
}
